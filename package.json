  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@types/express": "^4.17.6",
    "@types/node": "^13.11.1",
    "@types/node-sass": "^4.11.0",
    "babel-loader": "^8.1.0",
    "concurrently": "^5.1.0",
    "nodemon": "^2.0.3",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
